{
  "swagger": "2.0",
  "info": {
    "title": "Gerar Dados",
    "version": "1.0.0",
    "description": "API para geração de dados aleatórios e validação de CPF, CEP, e envio de SMS"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/gerar-dadosAleatorios": {
      "get": {
        "description": "Endpoint para gerar dados fictícios.",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/validar-cpf": {
      "post": {
        "description": "Endpoint para validar CPF.",
        "parameters": [
          {
            "name": "cpf",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "cpf": {
                  "type": "string",
                  "example": "12345678909"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "CPF válido"
          },
          "400": {
            "description": "CPF inválido"
          }
        }
      }
    },
    "/validar-cep": {
      "post": {
        "description": "Endpoint para validar CEP.",
        "parameters": [
          {
            "name": "cep",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "cep": {
                  "type": "string",
                  "example": "12345678"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "CEP válido"
          },
          "400": {
            "description": "CEP inválido"
          }
        }
      }
    },
    "/enviar-sms": {
      "post": {
        "description": "Endpoint para enviar SMS.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "to": {
                  "type": "string",
                  "example": "+1234567890"
                },
                "mensagem": {
                  "type": "string",
                  "example": "Mensagem de teste"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "SMS enviado com sucesso"
          },
          "400": {
            "description": "Erro ao enviar SMS"
          }
        }
      }
    },
    "/inserir-usuario": {
      "post": {
        "description": "Endpoint para inserir um novo usuário no MongoDB.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "login": {
                  "type": "string",
                  "example": "usuarioteste"
                },
                "senha": {
                  "type": "string",
                  "example": "senhateste"
                },
                "telefone": {
                  "type": "string",
                  "example": "+1234567890"
                },
                "endereco": {
                  "type": "string",
                  "example": "123 Main St, City"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Usuário inserido com sucesso"
          },
          "400": {
            "description": "Falha ao inserir usuário"
          },
          "500": {
            "description": "Erro interno do servidor"
          }
        }
      }
    }
  }
}
